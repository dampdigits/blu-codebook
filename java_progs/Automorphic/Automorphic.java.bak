class Automorphic//Automorphic No.: A no. whose square's last digit(s) is equal to the no. itself
{
    public static void main(long a)
    {
        long a1=a*a;
        long a2=a;
        int b=1;
        while(a2!=0)
        {
            a2/=10;
            b*=10;
        }
        if(a1%b==a)
        {
            System.out.println("\nIt is an automorphic no.");
        }
        else
        {
            System.out.println("\nIt is not an automorphic no.");
        }
    }
}